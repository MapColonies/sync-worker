{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "debug",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "tiles": {
    "path": "/",
    "format": "png",
    "sigIsNeeded": true
  },
  "crypto": {
    "readFileEncoding": "ascii",
    "pem": "/tmp/key.pem",
    "algoritm": "SHA512",
    "sigEncoding": "base64"
  },
  "queue": {
    "jobManagerBaseUrl": "http://localhost:8082",
    "heartbeatManagerBaseUrl": "http://localhost:8083",
    "dequeueIntervalMs": 5000,
    "heartbeatIntervalMs": 5000,
    "jobType": "Sync",
    "taskType": "Sync"
  },
  "db": {
    "host": "localhost",
    "user": "postgres",
    "password": "postgres",
    "database": "sync-storage",
    "port": 5432
  }
}
