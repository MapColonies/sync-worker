{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "layerSpecBaseUrl": "http://localhost:8080",
  "nifiBaseUrl": "http://localhost:8081",
  "syncAttempts": 5,
  "gateway": {
    "url": "http://localhost:8084"
  },
  "tiles": {
    "path": "/path/to/tiles/dir",
    "format": "png",
    "uploadBatchSize": 100,
    "sigIsNeeded": true
  },
  "crypto": {
    "readFileEncoding": "ascii",
    "pem": "/path/to/key.pem",
    "algoritm": "SHA512",
    "sigEncoding": "base64"
  },
  "queue": {
    "jobManagerBaseUrl": "http://localhost:8082",
    "heartbeatManagerBaseUrl": "http://localhost:8083",
    "dequeueIntervalMs": 5000,
    "heartbeatIntervalMs": 5000,
    "jobType": "Sync",
    "taskType": "Sync"
  },
  "httpRetry": {
    "attempts": 5,
    "delay": "exponential",
    "shouldResetTimeout": true
  }
}
