{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8088",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "layerSpecBaseUrl": "http://localhost:8080",
  "nifiBaseUrl": "http://localhost:8081",
  "syncAttempts": 5,
  "gateway": {
    "url": "http://localhost:8084",
    "imageRouteId": "",
    "jsonRouteId": ""
  },
  "tiles": {
    "path": "/path/to/tiles/directory",
    "format": "png",
    "uploadBatchSize": 100,
    "sigIsNeeded": true
  },
  "crypto": {
    "readFileEncoding": "ascii",
    "pem": "/path/to/private_key.pem",
    "shaSize": "sha512",
    "algoritm": "aes-256-cfb"
  },
  "queue": {
    "jobManagerBaseUrl": "http://localhost:8082",
    "heartbeatManagerBaseUrl": "http://localhost:8083",
    "dequeueIntervalMs": 1000,
    "heartbeatIntervalMs": 3000,
    "jobType": "Sync",
    "tilesTaskType": "tilesSync",
    "tocTaskType": "tocSync"
  },
  "httpRetry": {
    "attempts": 5,
    "delay": "exponential",
    "shouldResetTimeout": true
  },
  "storageProvider": "s3",
  "useStreams": true,
  "S3": {
    "accessKeyId": "minioadmin",
    "secretAccessKey": "minioadmin",
    "endpoint": "http://localhost:9000",
    "bucket": "mapping-images",
    "forcePathStyle": true
  }
}
