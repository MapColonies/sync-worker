{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "tiles": {
    "path": "TILES_PATH",
    "format": "TILES_FORMAT"
  },
  "signature": {
    "readFileEncoding": "SIGN_READ_FILE_ENCODING",
    "pem": "SIGN_PEM",
    "algoritm": "SIGN_ALOGRITM",
    "signEncoding": "SIGN_OUTPUT_ENCODING"
  },
  "queue": {
    "jobManagerBaseUrl": "QUEUE_JOB_MANAGER_BASE_URL",
    "heartbeatManagerBaseUrl": "QUEUE_HEART_BEAT_MANAGER_BASE_URL",
    "dequeueIntervalMs": {
      "__name": "QUEUE_DEQUEUE_INTERVAL_MS",
      "__format": "number"
    },
    "heartbeatIntervalMs": {
      "__name": "QUEUE_HEART_BEAT_INTERVAL_MS",
      "__format": "number"
    },
    "jobType": "QUEUE_JOB_TYPE",
    "taskType": "QUEUE_TASK_TYPE"
  },
  "db": {
    "host": "DB_HOST",
    "port": {
      "__name": "DB_PORT",
      "__format": "number"
    },
    "user": "DB_USER",
    "password": "DB_PASSWORD",
    "database": "DB_NAME",
    "enableSslAuth": {
      "__name": "DB_SSL_ENABLE",
      "__format": "boolean"
    },
    "rejectUnauthorized": {
      "__name": "DB_REJECT_UNAUTHORIZED",
      "__format": "boolean"
    },
    "sslPaths": {
      "ca": "DB_SSL_CA",
      "key": "DB_SSL_KEY",
      "cert": "DB_SSL_CERT"
    }
  }
}
